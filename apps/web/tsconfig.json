{
  "extends": "@cared/tsconfig/base.json",
  "compilerOptions": {
    "lib": [
      "ES2022",
      "dom",
      "dom.iterable"
    ],
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/public/*": [
        "./public/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "module": "esnext"
  },
  "include": ["src/**/*", "cloudflare-env.d.ts", "vite.config.ts"],
  "exclude": [
    "node_modules"
  ]
}
