{
  "name": "@cared/providers",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./embed": "./src/embed.ts",
    "./rerank": "./src/rerank/index.ts",
    "./providers": "./src/providers.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "extract-model-ids": "tsx ./scripts/extract-model-ids.ts",
    "fetch-openrouter-models": "pnpm with-env tsx ./scripts/fetch-openrouter-models.ts && pnpm prettier --check --write ./src/provider-info/openrouter.ts",
    "update-ai-dependencies": "pnpm update @ai-sdk/amazon-bedrock @ai-sdk/anthropic @ai-sdk/assemblyai @ai-sdk/azure @ai-sdk/cerebras @ai-sdk/cohere @ai-sdk/deepgram @ai-sdk/deepinfra @ai-sdk/deepseek @ai-sdk/elevenlabs @ai-sdk/fal @ai-sdk/fireworks @ai-sdk/gateway @ai-sdk/gladia @ai-sdk/google @ai-sdk/google-vertex @ai-sdk/groq @ai-sdk/hume @ai-sdk/lmnt @ai-sdk/luma @ai-sdk/mistral @ai-sdk/openai @ai-sdk/openai-compatible @ai-sdk/perplexity @ai-sdk/provider @ai-sdk/provider-utils @ai-sdk/replicate @ai-sdk/revai @ai-sdk/togetherai @ai-sdk/vercel @ai-sdk/xai @friendliai/ai-provider @openrouter/ai-sdk-provider @portkey-ai/vercel-provider @requesty/ai-sdk ai-gateway-provider cohere-ai dify-ai-provider",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^3.0.16",
    "@ai-sdk/anthropic": "^2.0.12",
    "@ai-sdk/assemblyai": "^1.0.8",
    "@ai-sdk/azure": "^2.0.25",
    "@ai-sdk/cerebras": "^1.0.15",
    "@ai-sdk/cohere": "^2.0.8",
    "@ai-sdk/deepgram": "^1.0.8",
    "@ai-sdk/deepinfra": "^1.0.15",
    "@ai-sdk/deepseek": "^1.0.15",
    "@ai-sdk/elevenlabs": "^1.0.9",
    "@ai-sdk/fal": "^1.0.12",
    "@ai-sdk/fireworks": "^1.0.15",
    "@ai-sdk/gateway": "^1.0.19",
    "@ai-sdk/gladia": "^1.0.8",
    "@ai-sdk/google": "^2.0.12",
    "@ai-sdk/google-vertex": "^3.0.21",
    "@ai-sdk/groq": "^2.0.17",
    "@ai-sdk/hume": "^1.0.8",
    "@ai-sdk/lmnt": "^1.0.8",
    "@ai-sdk/luma": "^1.0.8",
    "@ai-sdk/mistral": "^2.0.13",
    "@ai-sdk/openai": "^2.0.25",
    "@ai-sdk/openai-compatible": "^1.0.15",
    "@ai-sdk/perplexity": "^2.0.8",
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.8",
    "@ai-sdk/replicate": "^1.0.8",
    "@ai-sdk/revai": "^1.0.8",
    "@ai-sdk/togetherai": "^1.0.15",
    "@ai-sdk/vercel": "^1.0.15",
    "@ai-sdk/xai": "^2.0.16",
    "@cared/shared": "workspace:*",
    "@friendliai/ai-provider": "^0.2.6",
    "@google/genai": "^1.17.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@portkey-ai/vercel-provider": "^2.1.0",
    "@requesty/ai-sdk": "^0.0.9",
    "ai": "^5.0.38",
    "ai-gateway-provider": "^0.0.11",
    "ajv": "^8.17.1",
    "cohere-ai": "^7.18.1",
    "decimal.js": "^10.6.0",
    "dify-ai-provider": "^0.1.6",
    "openai": "^5.15.0"
  },
  "devDependencies": {
    "@cared/eslint-config": "workspace:*",
    "@cared/prettier-config": "workspace:*",
    "@cared/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "^4.19.3",
    "typescript": "catalog:"
  },
  "prettier": "@cared/prettier-config"
}
