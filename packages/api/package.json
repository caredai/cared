{
  "name": "@cared/api",
  "version": "0.1.9",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types/index.ts",
    "./auth": "./src/auth/index.ts",
    "./rest": "./src/rest/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "generate-api-types": "tsx ./scripts/generate-api-types.ts && pnpm prettier --check --write ../sdk/src/api.d.ts",
    "push-provider-models": "pnpm with-env tsx ./scripts/push-provider-models.ts",
    "load-redis-script": "pnpm with-env tsx ./scripts/load-redis-script.ts",
    "upload-tokenizer-assets": "pnpm with-env tsx ./scripts/upload-tokenizer-assets.ts",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@cared/auth": "workspace:*",
    "@cared/db": "workspace:*",
    "@cared/email": "workspace:*",
    "@cared/etl": "workspace:*",
    "@cared/kv": "workspace:*",
    "@cared/log": "workspace:*",
    "@cared/providers": "workspace:*",
    "@cared/shared": "workspace:*",
    "@cared/tokenizer": "workspace:*",
    "@cared/tools": "workspace:*",
    "@cared/validators": "workspace:*",
    "@cared/vdb": "workspace:*",
    "@langfuse/client": "^4.0.0",
    "@langfuse/otel": "^4.0.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@orpc/client": "^1.8.9",
    "@orpc/openapi": "^1.8.9",
    "@orpc/server": "^1.8.9",
    "@trpc/server": "catalog:",
    "@upstash/qstash": "^2.7.21",
    "@upstash/workflow": "^0.2.7",
    "mime": "^4.0.6",
    "next-s3-upload": "^0.3.4",
    "sanitize-filename": "^1.6.3",
    "stripe": "^18.3.0",
    "superjson": "2.2.2",
    "trpc-to-openapi": "^3.0.1",
    "zod": "catalog:",
    "zod-form-data": "^2.0.7"
  },
  "devDependencies": {
    "@cared/eslint-config": "workspace:*",
    "@cared/prettier-config": "workspace:*",
    "@cared/tsconfig": "workspace:*",
    "@commander-js/extra-typings": "^14.0.0",
    "@upstash/qstash-cli": "^2.21.1",
    "commander": "13.1.0",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@cared/prettier-config"
}
